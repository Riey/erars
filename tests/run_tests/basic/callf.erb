@SYSTEM_TITLE
LOCAL:5 = 11

FOR COUNT, 0, 9
    CALLF SET_LIST, COUNT, LOCAL:COUNT
NEXT

PRINTFORM {GET_LIST(5)}

@SET_LIST(ARG, ARG:1)
#FUNCTION
RETURNF LIST_DATA("SET", ARG, ARG:1)

@GET_LIST(ARG)
#FUNCTION
RETURNF LIST_DATA("GET", ARG)

@LIST_DATA(ARGS, ARG, ARG:1)
#FUNCTION
IF ARGS == "SET"
    LOCAL:ARG = ARG:1
    RETURNF ARG:1
ELSEIF ARGS == "GET"
    RETURNF LOCAL:ARG
ELSEIF ARGS == "CLEAR"
    VARSET LOCAL, 0
    RETURNF -1
ELSE
    RETURNF -1
ENDIF
